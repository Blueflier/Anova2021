{
    "manifest_version": 2,
  
    "name": "url getter",
    "description": "This extension will get the url in console",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": {
        "6": "Resized image/Default.png",
        "1":  "Resized image/A.png"
        },         
        "default_popup": "popup.html"
        },
        "permissions": [
        "activeTab"
        ]
  }


  